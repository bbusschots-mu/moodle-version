{
    "source": "./src",
    "destination": "./docs-dev",
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "accessor": { "access": ["public", "protected", "private"] },
                "autoPrivate": true,
                "unexportedIdentifier": {"enable": true}
            }
        },
        {"name": "esdoc-node"}
    ]
}