    "source": "./src",
    "destination": "./docs",
    "plugins": [
        {
            "name": "esdoc-standard-plugin",
            "option": {
                "accessor": { "access": ["public"] },
                "autoPrivate": true,
                "unexportedIdentifier": {"enable": false}
            }
        },
        {"name": "esdoc-node"}
    ]
}